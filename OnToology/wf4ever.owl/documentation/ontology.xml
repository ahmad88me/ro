    <!-- http://purl.org/wf4ever/wf4ever#WorkflowResearchObject -->


    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#WorkflowResearchObject">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/wf4ever/ro#ResearchObject"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.openarchives.org/ore/terms/aggregates"/>
