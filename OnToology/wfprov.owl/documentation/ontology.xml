        <rdfs:isDefinedBy rdf:resource="http://purl.org/wf4ever/wfdesc"/>
    </rdf:Description>
    


    <!-- http://purl.org/wf4ever/wfdesc#Workflow -->


    <rdf:Description rdf:about="http://purl.org/wf4ever/wfdesc#Workflow">
